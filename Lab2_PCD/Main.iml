import math

# Исходные данные
m = 2.14
n = 1.34

print(f"{'g':>5} | {'a':>12} | {'b':>12}")
print("-" * 36)

g = 1.0
while g <= 1.64 + 1e-9:  # небольшой запас, чтобы включить 1.64
    # Вычисления по формулам
    a = g**5 * (math.log10(n + m))**2 + m / math.sqrt(g) + n
    b = (n * g**3 - a) / (math.exp(a) - 7)

    # Вывод строки таблицы
    print(f"{g:5.2f} | {a:12.9f} | {b:12.9f}")
    g += 0.1
